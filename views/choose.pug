extends layout
block content
  h1= title
  - var image_names=[ "shoe1.webp","shoe2.webp","shoe3.webp","shoe4.jpeg","shoe5.webp"]
    -for(let i=0; i<3; i++){
      -let random= Math.floor(Math.random()*5)
       img(src=`/images/${image_names[random]}` alt=`Image No ${random}` height="300px" width="300px")
      - }